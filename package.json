{
  "name": "nodejs_uoc",
  "version": "1.0.0",
  "description": "Repositorio trabajos asignatura Herramientas HTML y CSS",
  "main": "src/index.html",
  "scripts": {
    "server": "live-server src --open",
    "parcel:dev": "parcel src/index.html",
    "parcel:build": "parcel build src/index.html",
    "dev": "npm-run-all clean parcel:dev",
    "build": "npm-run-all clean parcel:build",
    "clean": "rimraf dist .cache .cache-loader",
    "format": "prettier --write \"src/**/*.js\"",
    "lint": "eslint \"src/**/*.js\" --quiet",
    "test": "jest --passWithNoTests --silent"
  },
  "keywords": [
    "UOC",
    "HTML",
    "CSS",
    "Node js",
    "npm",
    "instancias",
    "github"
  ],
  "author": {
    "name": "Daniel Sariego Alvarez",
    "email": "danielsariego82@gmail.com"
  },
  "license": "ISC",
  "devDependencies": {
    "@babel/core": "^7.13.14",
    "@babel/preset-env": "^7.13.12",
    "autoprefixer": "^9.8.6",
    "babel-jest": "^26.6.3",
    "eslint": "^7.23.0",
    "eslint-config-prettier": "^8.1.0",
    "jest": "^26.6.3",
    "parcel-bundler": "^1.12.5",
    "prettier": "^2.2.1"
  },
  "dependencies": {
    "live-server": "^1.2.1",
    "npm-run-all": "^4.1.5"
  },
  "repository": {
    "type": "git",
    "url": "https://github.com/dsariego/dsariego",
    "remote": "https://github.com/dsariego/uoc.git"
  },
  "homepage": "https://github.com/dsariego/dsariego"
}
